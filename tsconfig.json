{
  "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "allowJs": true,
  "outDir": "./dist",
  "rootDir": "./src",
  "strict": true,
  "strictPropertyInitialization": false,
  "moduleResolution": "node",
  "esModuleInterop": true,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
  }
 }